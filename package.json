{
  "name": "hdi.ma",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "build": "npm run build:js ; npm run build:css",
    "start": "node app.js",
    "monitor": "nodemon app.js",
    "deploy": "npm run prestart && gcloud app deploy app.yaml",
    "build:js": "BABEL_ENV=default rollup -c .rollup/main.js",
    "prebuild:css": "sass public/css/main.scss public/css/styles.css",
    "build:css": "postcss public/css/styles.css -o public/css/styles.min.css",
    "mocha-app": "_mocha test/app/**/* --exit",
    "mocha-client": "BABEL_ENV=test _mocha --compilers js:babel-core/register test/client/**/*.js",
    "coverage": "istanbul cover _mocha --compilers js:babel-core/register test/app/**/*",
    "test": "npm run lint && npm run mocha-client && npm run mocha-app",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-minify-html": "^0.12.0",
    "express-sslify": "^1.2.0",
    "handlebars": "^4.7.6",
    "hbs": "^4.1.1",
    "http-parser-js": "^0.5.2",
    "moment": "^2.29.4",
    "nconf": "^0.10.0",
    "parse-color": "^1.0.0",
    "rev-hash": "^3.0.0",
    "serve-static": "^1.14.1",
    "sw-offline-google-analytics": "0.0.25",
    "sw-toolbox": "^3.6.0",
    "url-polyfill": "^1.1.9",
    "whatwg-fetch": "^3.0.0",
    "yaku": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-replace": "^2.3.2",
    "autoprefixer": "^9.8.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "postcss-cli": "^7.1.1",
    "prettier": "^2.0.5",
    "rollup": "^2.10.9",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "sass": "^1.26.5"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "cssnano": {}
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,md,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
