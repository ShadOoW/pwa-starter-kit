{
  "name": "hdi.ma",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node app.js",
    "prestart": "BABEL_ENV=default rollup -c .rollup/hdi.ma.js",
    "monitor": "nodemon app.js",
    "deploy": "npm run prestart && gcloud app deploy app.yaml",
    "build:sw:dev": "BABEL_ENV=sw rollup -c .rollup/sw.js --config-env=dev && node ./.workbox/inject.js",
    "build:sw:prod": "BABEL_ENV=sw rollup -c .rollup/sw.js --config-env=prod && node ./.workbox/inject.js",
    "mocha-app": "_mocha test/app/**/* --exit",
    "mocha-client": "BABEL_ENV=test _mocha --compilers js:babel-core/register test/client/**/*.js",
    "coverage": "istanbul cover _mocha --compilers js:babel-core/register test/app/**/*",
    "test": "npm run lint && npm run mocha-client && npm run mocha-app",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "escape-html": "^1.0.3",
    "express": "^4.17.1",
    "express-minify-html": "^0.12.0",
    "express-sslify": "^1.2.0",
    "handlebars": "^4.7.6",
    "hbs": "^4.1.1",
    "http-parser-js": "^0.5.2",
    "moment": "^2.26.0",
    "nconf": "^0.10.0",
    "parse-color": "^1.0.0",
    "rev-hash": "^3.0.0",
    "serve-static": "^1.14.1",
    "sw-offline-google-analytics": "0.0.25",
    "sw-toolbox": "^3.6.0",
    "url-polyfill": "^1.1.9",
    "whatwg-fetch": "^3.0.0",
    "workbox-core": "^5.1.3",
    "workbox-navigation-preload": "^5.1.3",
    "workbox-precaching": "^5.1.3",
    "workbox-routing": "^5.1.3",
    "workbox-strategies": "^5.1.3",
    "yaku": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@rollup/plugin-replace": "^2.3.2",
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "rollup": "^2.10.7",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-delete": "^1.2.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^6.0.1",
    "workbox-build": "^5.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{css,md,scss}": "prettier --write",
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
